{
  "name": "ngx-amap-demo",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=8080",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href ./",
    "build:lib": "ng build ngx-amap",
    "build:lib:prod": "ng build ngx-amap --prod",
    "test": "ng test",
    "test:lib": "ng test ngx-amap",
    "lint": "ng lint",
    "lint:lib": "ng lint ngx-amap",
    "e2e": "ng e2e",
    "compodoc": "compodoc -p projects/ngx-amap/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/ngx-amap/tsconfig.lib.json -s",
    "build:doc": "run-s build:prod compodoc",
    "update:file": "cpr README.md dist/ngx-amap/README.md -o",
    "release": "run-s build:lib:prod update:file build:doc",
    "demo": "run-s build:lib start",
    "dist": "run-s build:lib:prod update:file",
    "prettier": "prettier --config .prettierrc --write '{src,e2e,projects}/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "ng-zorro-antd": "^8.5.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular-devkit/build-ng-packagr": "^0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/amap-js-api": "^1.4.7",
    "@types/amap-js-api-autocomplete": "^1.4.0",
    "@types/amap-js-api-driving": "^1.4.0",
    "@types/amap-js-api-heatmap": "^1.4.0",
    "@types/amap-js-api-place-search": "^1.4.0",
    "@types/amap-js-api-tool-bar": "^1.4.0",
    "@types/amap-js-api-transfer": "^1.4.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "cpr": "^3.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^5.7.1",
    "ngx-markdown": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prismjs": "^1.27.0",
    "protractor": "^5.4.3",
    "raw-loader": "^3.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
